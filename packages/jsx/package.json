{
	"name": "@robino/jsx",
	"description": "jsx to html import source",
	"version": "0.0.11",
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.js",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./jsx-runtime": {
			"types": "./dist/jsx-runtime.d.ts",
			"default": "./dist/jsx-runtime.js"
		},
		"./jsx-dev-runtime": {
			"types": "./dist/jsx-dev-runtime.d.ts",
			"default": "./dist/jsx-dev-runtime.js"
		}
	},
	"license": "MIT",
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"check": "tsc --noEmit",
		"dev": "tsc --watch",
		"dev:nocheck": "tsc --watch --noCheck",
		"build": "tsc"
	},
	"dependencies": {
		"@robino/html": "^0.1.0"
	}
}
